                            <div class="image">
                                <img src="<%- discover.img %>" alt="">
                                <span class="badge bg-brown text-white text-center text-uppercase"></span>
                            </div>
                            <div class="info">
                                <div class="ratings text-grey">
                                    <i class="fas fa-star text-brown"></i>
                                    <i class="fas fa-star text-brown"></i>
                                    <i class="fas fa-star text-brown"></i>
                                    <i class="fas fa-star text-brown"></i>
                                    <i class="fas fa-star text-brown"></i>
                                    <span></span>
                                </div>
                                <p class="name"><%- discover.name %></p>

                            </div>
                        </div>

                        <% }) %>     
                        <% } %> 


                    </div>
                </div>
            </section>

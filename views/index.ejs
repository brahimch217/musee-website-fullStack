                        <div class="category-item">
                            <img src="<%- fild.img %>">
                            <div class="category-badge bg-white text-dark flex"><%- fild.name %></div>
                        </div>
                        <% }) %>     
                        <% } %> 
                       
                    </div>
                </div>
            </section>
            <!-- end of category section -->
